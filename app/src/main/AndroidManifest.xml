<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uts.findmeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".DetailTestimoni"></activity>
        <activity android:name=".TestimonyForm" />
        <activity android:name=".Profile_Activity" />
        <activity android:name=".PageFormLaporan_Activity" />
        <activity android:name=".PageDetailLaporan_Activity" />
        <activity android:name=".PageHelp4_Activity" />
        <activity android:name=".PageHelp3_Activity" />
        <activity android:name=".PageHelp2_Activity" />
        <activity android:name=".PageHelp1_Activity" />
        <activity android:name=".PageBantuan_Activity" />
        <activity android:name=".PageAboutUs_Activity" />
        <activity android:name=".MainNavi_Activity" />
        <activity android:name=".PageLogin_Activity" />
        <activity android:name=".PageDaftar_Activity" />
        <activity android:name=".PageAwal_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.android.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>